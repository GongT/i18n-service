{
	"compileOnSave": true,
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"lib": ["es6", "es5", "es7", "dom"],
		"moduleResolution": "node",
		"noEmitOnError": true,
		"noImplicitThis": true,
		"noImplicitAny": false,
		"removeComments": false,
		"preserveConstEnums": true,
		"experimentalDecorators": true,
		"declaration": true,
		"emitDecoratorMetadata": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": false,
		"pretty": true,
		"sourceMap": true,
		"inlineSources": true,
		"baseUrl": "../",
		"typeRoots": [],
		"types": [],
		"sourceRoot": "/data/node_modules/@gongt/i18n-client",
		"outDir": "../../dist/npm-package",
		"rootDir": "./",
		"paths": {
			"@gongt/ts-stl-client": ["node_modules/@gongt/ts-stl-client"],
			"@gongt/ts-stl-library": ["node_modules/@gongt/ts-stl-library"],
			"@gongt/ts-stl-server": ["node_modules/@gongt/ts-stl-server"]
		}
	}
}
